	"devDependencies": {
		"eslint"                         : "^8.31.0",
		"eslint-plugin-align-assignments": "^1.1.2" ,
		"eslint-plugin-eslint-comments"  : "^3.2.0" ,
		"eslint-plugin-jsdoc"            : "^39.6.4"
	},
	"dependencies": {"app-root-dir": "^1.0.2", "log4js": "^6.7.1"},
	"optionalDependencies": {"node-watch": "^0.7.3"}
