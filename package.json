{
	"name": "log4js-node-console",
	"version": "1.2.4",
	"description": "Compatibility layer between log4js and Node.js console",
	"homepage": "https://github.com/l0ner/log4js-node-console#readme",
	"bugs": {"url": "https://github.com/l0ner/log4js-node-console/issues"},
	"repository": "github:l0ner/log4js-node-console",
	"main": "src/log4js-node-console.js",
	"types": "types/log4js-node-console.d.ts",
